{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "K8sDeploy": {
    "Name": "id",
    "User": "19999",
    "Group": "19999",
    "Volumes": {
      "AppData": {
        "Source": "appdata",
        "Dest": "/appdata"
      }
    },
    "Secrets": {
      "IdCert": {
        "SecretName": "id-server-signing-cert",
        "Dest": "/run/secrets/id.signingcert.p12",
        "Type": "File"
      }
    },
    "RepoUrl": "https://github.com/threax/Threax.IdServer.git",
    "Branch": "RemoveCertManager",
    "Dockerfile": "Threax.IdServer/Dockerfile",
    "InitCommand": "dotnet Threax.IdServer.dll tools migrate seed"
  }
}
